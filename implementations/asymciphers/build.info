$SM2_GOAL=../../libdefault.a
$HSM_SM2_GOAL=../../libdefault.a

SOURCE[$RSA_GOAL]=rsa_enc.c

SOURCE[$HSM_SM2_GOAL]=hsm_sm2.c

IF[{- !$disabled{"sm2"} -}]
  SOURCE[$SM2_GOAL]=sm2_enc.c
ENDIF
